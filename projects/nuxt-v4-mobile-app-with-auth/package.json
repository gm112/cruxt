{
  "name": "nuxt-v4-mobile-app-with-auth",
  "private": true,
  "type": "module",
  "config": {
    "environments": ["development"],
    "tags": ["internal"],
    "database_type": "postgresql",
    "cloud_provider": "aws-amplify"
  },
  "scripts": {
    "prepare": "nuxt prepare",
    "build": "nuxt build",
    "dev": "cp -f .env.example .env && nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "deploy:deno": "mv .output ../../"
  },
  "dependencies": {
    "nuxt": "catalog:"
  },
  "devDependencies": {
    "@cruxt/auth": "workspace:*",
    "@capacitor/android": "catalog:",
    "@capacitor/assets": "catalog:",
    "@capacitor/cli": "catalog:",
    "@capacitor/core": "catalog:",
    "@capacitor/dialog": "catalog:",
    "@capacitor/geolocation": "catalog:",
    "@capacitor/google-maps": "catalog:",
    "@capacitor/ios": "catalog:",
    "@capacitor/local-notifications": "catalog:",
    "@capacitor/preferences": "catalog:",
    "@capacitor/push-notifications": "catalog:",
    "@capacitor/splash-screen": "catalog:",
    "@capacitor/toast": "catalog:",
    "@transistorsoft/capacitor-background-fetch": "catalog:",
    "@transistorsoft/capacitor-background-geolocation": "catalog:",
    "@trapezedev/project": "catalog:"
  }
}
